{
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "import/no-extraneous-dependencies": 0,
    "max-len": 0,
    "new-cap": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-unused-vars": 1,
    "react/jsx-filename-extension": 0,
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "semi": 0
  }
}
